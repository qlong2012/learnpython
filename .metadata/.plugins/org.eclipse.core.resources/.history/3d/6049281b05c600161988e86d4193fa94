import random

#生成信用卡ID，随机生成6个数字，组成卡号，返回卡号
def creditcard_id():
    card_id = [] 
    for i in range(6):
        num = randdom.randint()
        card_id.append(num)
    card_id = ''.join(card_id)
    print(card_id)
    return card_id
        
        
